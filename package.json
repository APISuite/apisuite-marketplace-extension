{
  "name": "apisuite-marketplace-extension",
  "version": "1.8.1",
  "description": "",
  "scripts": {
    "server": "ts-node dist/src/server/index.js",
    "consumer": "ts-node dist/src/msg-broker/index.js",
    "dev-server": "nodemon --watch 'src/**/*.ts' --exec \"ts-node\" src/server/index.ts",
    "dev-consumer": "nodemon --watch 'src/**/*.ts' --exec \"ts-node\" src/msg-broker/index.ts",
    "lint": "eslint 'src/**/*.ts'",
    "lint-api": "spectral lint openapi.yml --ruleset=https://raw.githubusercontent.com/Cloudoki/openapi-style-guide/main/spec.yaml",
    "build": "tsc -p .",
    "test": "NODE_ENV=test LOG_LEVEL=silent mocha -r ts-node/register src/**/*.spec.ts",
    "migrate": "knex migrate:latest --client pg --migrations-directory ./migrations --connection \"$DB_URI\""
  },
  "license": "MPL-2.0",
  "dependencies": {
    "amqp-connection-manager": "3.2.2",
    "amqplib": "0.8.0",
    "camelcase-keys": "6.2.2",
    "convict": "6.1.0",
    "cors": "2.8.5",
    "express": "4.17.1",
    "express-basic-auth": "1.2.1",
    "express-prom-bundle": "6.3.6",
    "helmet": "4.6.0",
    "knex": "0.95.6",
    "morgan": "1.10.0",
    "pg": "8.6.0",
    "pino": "6.11.3",
    "prom-client": "13.1.0",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@semantic-release/git": "^9.0.0",
    "@stoplight/spectral": "^5.9.1",
    "@types/amqp-connection-manager": "^2.0.10",
    "@types/amqplib": "^0.8.0",
    "@types/chai": "^4.2.18",
    "@types/convict": "^6.0.2",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.12",
    "@types/mocha": "^8.2.2",
    "@types/morgan": "^1.9.2",
    "@types/node": "^15.12.2",
    "@types/node-fetch": "^2.5.8",
    "@types/pg": "^8.6.0",
    "@types/pino": "^6.3.8",
    "@types/sinon": "^10.0.2",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "chai": "^4.3.4",
    "eslint": "^7.28.0",
    "mocha": "^9.0.0",
    "nodemon": "^2.0.7",
    "semantic-release": "^17.4.4",
    "sinon": "^11.1.1",
    "supertest": "^6.1.3",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2"
  },
  "release": {
    "branches": [
      {
        "name": "main"
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json"
          ],
          "message": "chore(release): ${nextRelease.version} [ci skip]"
        }
      ],
      "@semantic-release/github"
    ]
  }
}
